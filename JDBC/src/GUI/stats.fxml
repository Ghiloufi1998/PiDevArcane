<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="572.0" prefWidth="710.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.StatsController">
   <children>
      <AnchorPane fx:id="stats" layoutX="76.0" layoutY="26.0" prefHeight="434.0" prefWidth="558.0">
         <children>
            <BarChart fx:id="bar" layoutX="14.0" layoutY="17.0" title="Stats">
              <xAxis>
                <CategoryAxis fx:id="valeurs" label="Facteur" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="count" label="Count" side="LEFT" />
              </yAxis>
            </BarChart>
         </children>
      </AnchorPane>
      <Button fx:id="button" layoutX="307.0" layoutY="490.0" mnemonicParsing="false" onAction="#affichstats" text="Afficher Stats" />
   </children>
</AnchorPane>
